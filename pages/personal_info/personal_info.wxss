.page{
  height: 100%;
  width: 100%;
  padding:10rpx;
}

.title{
  font-size:32rpx;
  font-weight: bold;
  padding:20rpx;
  text-align:center;
}

.l-form-btn-class{
  display: flex;
  justify-content: space-around;
  margin: 10rpx;
}

.image{
  margin-left: 35rpx;
  margin-top:20rpx;
  margin-bottom: 20rpx;
}

.radio{
  margin-left: 35rpx;
}

/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked{
  background: #86ff9c !important;
}

/*头像*/
.image{
  width: 135rpx;
  height: 135rpx;
  background: #FFFFFF;
  border: 2rpx solid #E5E5E5;
  border-radius: 50%;
}
.image image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

l-input{
  font-size:20rpx !important;
}