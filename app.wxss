/**app.wxss**/
@import "./styles/iconfont.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/*主题颜色，通过变量实现*/
page{
  /*主题颜色*/
  --themeColor:#86ff9c;
}


